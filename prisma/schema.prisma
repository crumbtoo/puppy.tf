// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id       String  @id
  tf2      String?
  csgo     String?
  r6s      String?
  nextCsgo String @unique
}

model Session {
  id        String   @id
  sid       String   @unique
  data      String
  expiresAt DateTime
}