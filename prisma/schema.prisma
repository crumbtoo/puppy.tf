// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id      Int     @id
  session String  @unique
}

model Session {
  id        String   @id
  sid       String   @unique
  data      String
  expiresAt DateTime
}